    <PackageReference Include="AWSSDK.SimpleNotificationService" Version="3.7.300.41" />
    <PackageReference Include="AWSSDK.SSO" Version="3.7.300.41" />
    <PackageReference Include="AWSSDK.SSOAdmin" Version="3.7.302.36" />
    <PackageReference Include="AWSSDK.SSOOIDC" Version="3.7.301.36" />
    <PackageReference Include="Microsoft.AspNet.WebApi.Core" Version="5.2.9" />
    <PackageReference Include="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" Version="1.18.1" />
    <PackageReference Include="NLog" Version="5.2.8" />
    <PackageReference Include="NLog.Extensions.Logging" Version="5.3.8" />
    <PackageReference Include="NLog.Web.AspNetCore" Version="5.3.8" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.5.0" />
  </ItemGroup>

